# Development Environment Configuration

# Application Environment
NODE_ENV=development
LOG_LEVEL=DEBUG

# Database Configuration
DATABASE_URL=postgresql://postgres:devpassword@postgres:5432/multi_bot_rag_dev
POSTGRES_DB=multi_bot_rag_dev
POSTGRES_USER=postgres
POSTGRES_PASSWORD=devpassword

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_PASSWORD=

# Vector Store Configuration
VECTOR_STORE_TYPE=qdrant
QDRANT_URL=http://qdrant:6333
QDRANT_LOG_LEVEL=DEBUG

# Security Configuration (Development - NOT for production!)
SECRET_KEY=dev-secret-key-change-in-production-very-long-key-for-development-only
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# File Storage Configuration
UPLOAD_DIR=/app/uploads
MAX_FILE_SIZE=10485760

# CORS Configuration
FRONTEND_URL=http://localhost:3000

# Frontend Environment Variables
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000
VITE_APP_NAME=Multi-Bot RAG Platform (Dev)

# Development Features
ENABLE_DEBUG_TOOLBAR=true
ENABLE_CORS_ALL_ORIGINS=true
ENABLE_DETAILED_ERRORS=true

# Resource Limits (Development)
BACKEND_WORKERS=1
BACKEND_MEMORY_LIMIT=512M
BACKEND_CPU_LIMIT=1.0

# Default Admin Configuration (Development Only)
DEFAULT_ADMIN_EMAIL=admin@dev.local
DEFAULT_ADMIN_PASSWORD=admin123